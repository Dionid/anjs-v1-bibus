  "main": "build/apps/main-gql/index.js",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-resolvers": "^1.19.5",
    "@types/cron": "^1.7.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.11",
    "@types/pg": "^8.6.5",
    "@types/prompt": "^1.1.1",
    "@types/uuid": "^8.3.0",
    "env-cmd": "^10.1.0",
    "hasura-cli": "^2.0.9",
    "introspect-it": "0.2.0",
    "jest": "^27.0.6",
    "jest-mock-extended": "^2.0.4",
    "json-schema-to-ts": "^1.6.5",
    "rimraf": "^3.0.2",
    "schemats": "^3.0.3",
    "testcontainers": "^7.13.0",
    "ts-jest": "^27.0.3",
    "ts-mockito": "^2.6.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/exporter-zipkin": "^1.0.1",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/instrumentation-net": "^0.27.1",
    "@opentelemetry/instrumentation-pg": "^0.28.0",
    "@opentelemetry/propagator-jaeger": "^1.0.1",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/sdk-trace-node": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "apollo-server": "^3.4.0",
    "cron": "^1.8.2",
    "dotenv": "^10.0.0",
    "fastify": "^3.27.2",
    "fastify-swagger": "^4.15.0",
    "graphql": "^15.6.1",
    "graphql-import-node": "^0.0.4",
    "graphql-tools": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "opentelemetry-instrumentation-typeorm": "^0.27.1",
    "pg": "^8.7.1",
    "pino": "^7.8.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.0",
    "typeorm": "^0.2.43",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "NODE_PATH=./build node --enable-source-maps build/apps/main-gql/index.js",
    "test": "jest --passWithNoTests",
    "introspect": "NODE_TLS_REJECT_UNAUTHORIZED=0 env-cmd -f src/apps/krusa/.env introspect-it > src/libs/@bibus/the-king/introspect-it-schema.ts",
    "gen:gql": "graphql-codegen --config codegen.yaml",
    "build:commander": "rimraf build && npm run introspect && tsc -P tsconfig.build.json",
    "build:krusa": "rimraf build && npm run introspect && npm run gen:gql && tsc -P tsconfig.build.json",
    "dev:commander": "npm run build:commander && NODE_PATH=./build node build/apps/commander/index.js",
    "dev:krusa": "npm run build:krusa && NODE_PATH=./build node build/apps/krusa/index.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(unit|int).ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^modules/(.*)$": "<rootDir>/src/modules/$1",
      "^apps/(.*)$": "<rootDir>/src/apps/$1",
      "^libs/(.*)$": "<rootDir>/src/libs/$1",
      "^commands/(.*)$": "<rootDir>/src/commands/$1",
      "^queries/(.*)$": "<rootDir>/src/queries/$1"
    },
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "engines": {
    "node": "16.x"
  }
}
